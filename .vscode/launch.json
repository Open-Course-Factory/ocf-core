    "version": "0.2.0",
    "configurations": [

    
        {
            "name": "Launch API",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "env": {}
        },
        {
            "name": "Launch Course Gen",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["-c", "golang", "-t", "sdv", "-e", "html", "--dry-run"],
            "env": {}
        },
        {
            "name": "Launch Downloaded Course Gen",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": ["-c", "git@gitlab.com/open-course-factory/ocf-course-golang-beginner.git", "-t", "sdv", "-e", "html", "--dry-run"],
            "env": {}
        }
    ]
}